<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">

        <record id="mt_doc_submit_request" model="mail.message.subtype">
            <field name="name">State Changes</field>
            <field name="default" eval="True"/>
            <field name="res_model">dha.document.submit</field>
            <field name="description">Document State Changes</field>
        </record>

        <record id="mt_doc_submit_request_received" model="mail.message.subtype">
            <field name="name">Document Received</field>
            <field name="default" eval="False"/>
            <field name="hidden" eval="True"/>
            <field name="res_model">dha.document.submit</field>
            <field name="description">Document State Received</field>
        </record>

    </data>
</odoo>