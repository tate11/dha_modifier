<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="seq_medical_bill" model="ir.sequence">
            <field name="name">Medical Bill</field>
            <field name="code">medic.medical.bill</field>
            <field name="prefix">BA</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">7</field>
        </record>

        <record id="seq_medic_appoint" model="ir.sequence">
            <field name="name">Medic Appoint</field>
            <field name="code">medic.appoint.seq</field>
            <field name="prefix">CD</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">7</field>
        </record>

        <record id="seq_medicine_bill" model="ir.sequence">
            <field name="name">Medicine Order</field>
            <field name="code">medicine.order.code</field>
            <field name="prefix">TH</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">7</field>
        </record>

            <!-- Medic Test Seq -->
        <record id="seq_medic_test_no_type" model="ir.sequence">
            <field name="name">Medic Test No Type</field>
            <field name="code">no.type.test.seq</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">7</field>
        </record>

        <record id="seq_medic_test_image_test" model="ir.sequence">
            <field name="name">Medic Test Image Test</field>
            <field name="code">image.test.seq</field>
            <field name="prefix">XQ</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">7</field>
        </record>

        <record id="seq_medic_test_lab_test" model="ir.sequence">
            <field name="name">Medic Test LAB Test</field>
            <field name="code">lab.test.seq</field>
            <field name="prefix">XN</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">7</field>
        </record>

        <record id="seq_medic_test_echograph" model="ir.sequence">
            <field name="name">Medic Test Echograph</field>
            <field name="code">echograph.test.seq</field>
            <field name="prefix">SA</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">7</field>
        </record>

        <record id="seq_medic_test_electrocardiogram" model="ir.sequence">
            <field name="name">Medic Test Electrocardiogram</field>
            <field name="code">electrocardiogram.test.seq</field>
            <field name="prefix">TD</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">7</field>
        </record>

        <!--sequence for customer id-->
        <record id="seq_medic_customer_id" model="ir.sequence">
            <field name="name">Customer ID</field>
            <field name="code">customer.id.seq</field>
            <field name="prefix"></field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">6</field>
        </record>

    </data>
</odoo>